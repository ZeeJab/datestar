<% @user.photos.each do |p| %>
<%= image_tag p.photo %>
<% end %>


<%= form_for @p, html: {multipart: true} do |f| %>
	<%= f.label :photo %>
	<%= f.file_field :photo  %>

		<%= f.hidden_field :user_id, :value => @authenticated_user.id if @authenticated_user %>

	<%= f.submit :upload %>
<%end %>

<%= link_to 'message me', match_path %>